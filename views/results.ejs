<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= query %></title>
</head>
<body>
    <h1>Results for "<%= query %>"</h1>
    <% results.forEach(result => { %>
        <p><%= result.domain %></p>
        <h2><%= result.title||result.domain %></h2>
        <h3><%= result.description %></h3>
    <% }) %>
</body>
</html>